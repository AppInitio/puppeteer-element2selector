{
  "name": "puppeteer-element2selector",
  "version": "0.0.1",
  "description": "A utility to convert puppeteer ElementHandle to CSS Selector.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "yarn build:ts && yarn build:bridge",
    "build:ts": "tsc",
    "build:bridge": "rollup -c",
    "test": "jest",
    "format": "prettier --write \"**/*.+(js|css|md|yml|json)\"",
    "prebuild": "yarn clean",
    "pretest": "yarn build",
    "release": "standard-version"
  },
  "keywords": [
    "puppeteer",
    "css",
    "selector",
    "generator"
  ],
  "author": "wadackel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wadackel/puppeteer-element2selector.git"
  },
  "homepage": "https://github.com/wadackel/puppeteer-element2selector#readme",
  "bugs": {
    "url": "https://github.com/wadackel/puppeteer-element2selector/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "puppeteer": ">= 2.1.0"
  },
  "dependencies": {
    "@medv/finder": "^1.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.1.3",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/puppeteer": "^2.0.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "puppeteer": "^2.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "serve-handler": "^6.1.2",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,yml,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
